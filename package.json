{
  "name": "reverb-ml",
  "productName": "reverb-ml",
  "version": "0.4.2",
  "description": "An Electron app to play, visualize, and annotate your audio files for machine learning",
  "main": "src/index.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "tslint --project tsconfig.json --type-check --force",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,md,css,scss}'"
  },
  "keywords": [
    "audio",
    "labelling",
    "visualization"
  ],
  "author": "Evan Louie <evan.louie@microsoft.com>",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "reverb_ml"
      },
      "electronInstallerDebian": {},
      "github_repository": {
        "owner": "evanlouie",
        "name": "reverb-ml"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "reverbml"
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^3.0.1",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^7.0.0",
    "immutable": "4.0.0-rc.10",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "reflect-metadata": "^0.1.12",
    "sqlite3": "^4.0.2",
    "typeorm": "^0.2.7",
    "wavesurfer.js": "^2.1.0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.11.3",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "3.0.0",
    "prettier": "^1.14.3",
    "tslint": "^5.1.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "~3.1.1"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": false
  }
}
